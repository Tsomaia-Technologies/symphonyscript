{
  "name": "@symphonyscript/core",
  "version": "0.1.0",
  "description": "SymphonyScript DSL, Compiler, and Music Theory",
  "license": "MIT",
  "author": "Tsomaia Technologies",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": "./dist/index.umd.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts && esbuild src/index.ts --bundle --format=iife --global-name=SymphonyScript --outfile=dist/index.umd.js",
    "test": "jest --config jest.config.js"
  }
}
