{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "src/**/*",
    "vite.config.ts",
    "../core/src/**/*",
    "../runtime-webaudio/src/**/*",
    "../live/src/**/*",
    "../midi-backend-web/src/**/*"
  ]
}


