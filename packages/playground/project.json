{
  "name": "playground",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite",
        "cwd": "packages/playground"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite build",
        "cwd": "packages/playground"
      },
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite preview",
        "cwd": "packages/playground"
      }
    }
  }
}
